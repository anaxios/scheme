<table border="0" cellpadding="0" cellspacing="0"><tr><td>
<div style="border:1px #666 solid;">
<script type="text/javascript">
//---------------------------------------------------------------------------------------------------------------------
// The code below instantiates GRC's ultra-high entropy pseudo-random number generator. Upon instantiation, the uheprng
// is initially randomized with the state of the browser's JavaScript prng. We then further enhance the uheprng entropy
// by adding the top-of-document time-of-day and 1536 bits of high-quality entropy provided by GRC's servers.
//
// The code below dynamically 'document.write's the Latin Square grid into the web page as the page is being displayed.
// The trickiest thing being done is that the 'oElementIDs' and 'oBorderIDs' arrays (defined below) are populated on-
// the-fly by defining a CSS ID tag of 'temp', using 'getElementById' to obtain the newly created element's object ID,
// then immediately using '.removeAttribute' to remove the just-assigned id='temp'.  This keeps the DOM small and also
// eliminates any need for different IDs and for duplicate IDs.
//---------------------------------------------------------------------------------------------------------------------
serverEntropy = "S36blMZpzjz7b+ajAiyN8aUqw+J7+DS+yBBr9n58EG26hjiE4D9YLy/yQ79I7RtVtrqfWICHcajPsd67YfLxq9s8lV1YdWMW/4zP1zF2qSbKonVB6j5jHwlmz9U3k4SiDvWF2S2iWXQvp2iNb3GYwIUgO0VBmEOYU1ylpbxBYCfPAcfsYKZ3Jy3WJ7jdu8A4kzv49toapH/QfOiqp8ZrfXGsFMhdrjvNl6fTKgPjzKqOf8+k84FYK00XgTE1Kh6r";						// initial high-quality seed entropy provided by GRC's server
instantiateUheprng();						// instantiate our UHEPRNG and initialize it with server and client entropy
//---------------------------------------------------------------------------------------------------------------------
Dim = 26;										// this is the master dimention (order) of the Latin Squares we'll be generating
oElementIDs = new Array ( Dim*Dim );	// declare the global array for access to our visual grid elements
oBorderIDs	= new Array ( Dim*4 );		// declare the global array for modifying the fonts used around the grid border
oSpacerIDs  = new Array ( 4 );			// declare the global array for modifying the 'display' property of our spacerTRs
//---------------------------------------------------------------------------------------------------------------------
(function() { 		// create the fully "id-ized" grid for access to its individual cells
	var i=0,j=0,k=0,n,c;	// 'i','j'&'k' generate unique IDs for the border and inner grid 'td's, and are autoincremented when they are used
	// put out our opening 'table' tag...
	document.write ( '<table class="table" border="0" cellpadding="0" cellspacing="0">' );
	// put out the top horizontal row of top special symbols
	document.write ( '<tr id="temp" class="spacertr"><td class="bordertd"></td><td class="border2"><img src="/image/transpixel.gif" alt="" /></td>' );
	(oSpacerIDs[ k++ ] = document.getElementById( 'temp' )).removeAttribute('id');
	for ( n = 0; n < Dim; n++ ) {
		document.write ( '<td id="temp" class="' + ( n<7 || n>18 ? 'bordertd' : 'domaintd' ) + '">&nbsp;</td>' );
		(oBorderIDs[ i++ ] = document.getElementById( 'temp' )).removeAttribute('id');
	}
	document.write ( '<td class="border2"><img src="/image/transpixel.gif" alt="" /></td><td class="bordertd"></td></tr>' );
	// put out the spacer row
	document.write ( '<tr id="temp" class="spacertr"><td class="border2"><img src="/image/transpixel.gif" alt="" /></td>' );
	(oSpacerIDs[ k++ ] = document.getElementById( 'temp' )).removeAttribute('id');
	for ( n = 0; n < Dim+2; n++ ) document.write ( '<td class="spacertd"></td>' );
	document.write ( '<td class="border2"><img src="/image/transpixel.gif" alt="" /></td></tr>' );
	// put out our entire central grid region
	for ( n = 0; n < Dim; n++ ) {
		// put out the central body of the table
		document.write ( '<tr>' );
		for ( c = 0; c < Dim+4; c++ ) {
			if ( c == 0 ) {
				document.write ( '<td id="temp" class="bordertd" style="border-right:1px;">&nbsp;</td>' );
				(oBorderIDs[ i++ ] = document.getElementById( 'temp' )).removeAttribute('id');
			} else if ( c > 1 && c < Dim+2 ) {
				var charclass =  n%2 && n<Dim-1 ? (c%4<2 ? 'char3' : 'char2') : (c%4<2 ? 'char1' : 'char0');
				// create a new 'td' tag with the id=temp within the grid
				document.write ( '<td id="temp" class="globaltd ' + charclass + '">&nbsp;</td>' );
				// grab and store the 'td's object value and reset its id to ''
				(oElementIDs[ j++ ] = document.getElementById( 'temp' )).removeAttribute('id');
			} else if ( c == Dim+3 ) {
				document.write ( '<td id="temp" class="bordertd" style="border-left:1px;">&nbsp;</td>' );
				(oBorderIDs[ i++ ] = document.getElementById( 'temp' )).removeAttribute('id');
			} else {
				// this is our spacer 'td'
				document.write ( '<td class="spacertd"></td>' );
			}
		}
		document.write ( '</tr>' );
	}
	// put out the spacer row	
	document.write ( '<tr id="temp" class="spacertr"><td class="border2"><img src="/image/transpixel.gif" alt="" /></td>' );
	(oSpacerIDs[ k++ ] = document.getElementById( 'temp' )).removeAttribute('id');
	for ( n = 0; n < Dim+2; n++ ) document.write ( '<td class="spacertd"></td>' );
	document.write ( '<td class="border2"><img src="/image/transpixel.gif" alt="" /></td></tr>' );
	// put out the bottom horizontal row of top special symbols
	document.write ( '<tr id="temp" class="spacertr"><td class="border2"></td><td class="border2"><img src="/image/transpixel.gif" alt="" /></td>' );
	(oSpacerIDs[ k++ ] = document.getElementById( 'temp' )).removeAttribute('id');
	for ( n = 0; n < Dim; n++ ) {
		document.write ( '<td  id="temp" class="bordertd">&nbsp;</td>' );
		(oBorderIDs[ i++ ] = document.getElementById( 'temp' )).removeAttribute('id');
	}
	document.write ( '<td class="border2"><img src="/image/transpixel.gif" alt="" /></td><td class="border2"></td></tr>' );
	document.write ( '</table>' );	// write out our table closer
})();
</script></div></td></tr></table>

