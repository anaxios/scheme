(define make-matrix
  (lambda (cols rows)
    (if (= rows cols)
	'()
	(cons (make-col cols
			rows)
	      (make-matrix cols
			   (+ rows 1))))))

(define (make-col n x)
    (if (< n 1)
      '()
      (cons (modulo x 25)
	    (make-col (- n 1) (+ x 1)))))

